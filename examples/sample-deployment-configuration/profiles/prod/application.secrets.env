# Application Secrets - Production Environment
# This file contains SOPS-encrypted secrets
# Format: KEY=VALUE

# Database Credentials
DATABASE_URL=postgresql://user:password@db-prod.example.com:5432/myapp_prod
DATABASE_USER=myapp_prod_user
DATABASE_PASSWORD=prod-secret-password-secure

# API Keys
API_KEY=prod-api-key-secure-xyz789
EXTERNAL_API_KEY=external-prod-key-secure-123

# OAuth Credentials
OAUTH_CLIENT_ID=prod-client-id-secure
OAUTH_CLIENT_SECRET=prod-client-secret-secure

# Encryption Keys
ENCRYPTION_KEY=prod-encryption-key-32-chars-long!!

# Service Account Keys (JSON format - single line)
SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"prod-project"}

